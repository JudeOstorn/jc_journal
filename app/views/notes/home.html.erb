<p id="notice"><%= notice %></p>

<h1>HOME</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%=link_to note.title, note %></td>
        <td><%= note.content %></td>
        <td><%#= link_to 'Show', note %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

